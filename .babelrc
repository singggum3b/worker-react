{
  "presets": [
    ["react"],
    ["env", {
      "targets": {
        "browsers": ["last 2 versions", "safari >= 9", "not ie < 11"]
      },
      "modules": false,
      "includes": ["transform-class-properties", "transform-object-rest-spread"],
      "useBuiltIns": true
    }]
  ],
  "plugins": ["transform-decorators-legacy", "transform-class-properties", "transform-object-rest-spread"],
  "ignore": ["node_modules/mobx-react-devtools"]
}
